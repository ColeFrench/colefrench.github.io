{
  "name": "colefrench-github-io",
  "version": "1.0.0",
  "description": "My personal website where you can read all about me and the things I enjoy doing.",
  "homepage": "https://github.com/ColeFrench/colefrench.github.io#readme",
  "bugs": {
    "url": "https://github.com/ColeFrench/colefrench.github.io/issues"
  },
  "license": "MIT",
  "author": "Cole French",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ColeFrench/colefrench.github.io.git"
  },
  "scripts": {
    "build:css": "build-css.sh",
    "build:js": "build-js.sh",
    "build": "build-css.sh && build-js.sh",
    "clean:css": "rimraf **/assets/build/css/",
    "clean:js": "rimraf **/assets/build/js/",
    "clean": "rimraf **/assets/build/",
    "update:css": "npm run clean:css && npm run build:css",
    "update:js": "npm run clean:js && npm run build:js",
    "update": "npm run update:css && npm run update:js",
    "watch:css": "onchange \"**/assets/css/*.css\" -- npm run update:css",
    "watch:js": "onchange \"**/assets/js/*.js\" -- npm run update:js",
    "watch": "concurrently \"npm run watch:css\" \"npm run watch:js\" -k"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "clean-css-cli": "^4.1.3",
    "concurrently": "^3.4.0",
    "mkdirp": "^0.5.1",
    "onchange": "^3.2.1",
    "postcss-cli": "^4.0.0",
    "rimraf": "^2.6.1",
    "uglify-js": "^3.0.9"
  },
  "private": true
}
